 # Налаштування середовища
NODE_ENV=production

# Налаштування CORS
FRONTEND_URL=https://train-schedule.example.com

# Налаштування логування
LOG_LEVEL=info

# База даних PostgreSQL
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/train_schedule

# Налаштування Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Налаштування JWT
JWT_SECRET=YourVeryLongAndComplexSecretKey123!@#$%^&*()_+
JWT_EXPIRES_IN=15m
JWT_REFRESH_SECRET=AnotherVeryLongAndComplexRefreshKey456!@#$%^&*()_+
JWT_REFRESH_EXPIRES_IN=7d